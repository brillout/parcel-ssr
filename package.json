{
  "scripts": {
    "build": "yarn clean && parcel build .",
    "clean": "rm -rf .parcel-cache/ && rm -rf dist"
  },
  "devDependencies": {
    "core-js": "2"
  },
  "dependencies": {
    "parcel": "2.0.0-alpha.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "source": "./pages/LandingPage.js",
  "main": "dist/LandingPage.node.js",
  "browser": "dist/LandingPage.browser.js",
  "targets": {
    "browser": {
      "includeNodeModules": true,
      "outputFormat": "global",
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    },
    "main": {
      "engines": {
        "node": ">=10.x"
      }
    }
  }
}
