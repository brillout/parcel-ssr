{
  "scripts": {
    "start": "yarn build && yarn start:server",
    "build": "yarn clean && yarn build:pages",
    "build:pages": "parcel build .",
    "clean": "rm -rf .parcel-cache/ && rm -rf dist/",
    "start:server": "node ./server"
  },
  "devDependencies": {
    "core-js": "2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "parcel": "2.0.0-alpha.2.1",
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
  "source": "./pages/landing.page.js",
  "main": "dist/landing.page.node.js",
  "browser": "dist/landing.page.browser.js",
  "targets": {
    "browser": {
      "includeNodeModules": true,
      "outputFormat": "global",
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    },
    "main": {
      "engines": {
        "node": ">=10.x"
      }
    }
  }
}

