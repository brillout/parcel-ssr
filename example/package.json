{
  "scripts": {
    "build": "yarn build:clean && yarn build:pages",
    "build:pages": "parcel build .",
    "build:clean": "rm -rf ../.parcel-cache/ && rm -rf dist/"
  },
  "devDependencies": {
    "core-js": "3",
    "parcel": "2.0.0-alpha.3.1"
  },
  "source": "./hello.js",
  "main": "dist/hello.node.js",
  "browser": "dist/hello.browser.js",
  "targets": {
    "browser": {
      "includeNodeModules": true,
      "outputFormat": "global",
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    },
    "main": {
      "engines": {
        "node": ">=10.x"
      }
    }
  },
  "name": "example",
  "version": "0.0.1",
  "private": true
}
